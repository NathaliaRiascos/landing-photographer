{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/sections/*": ["./src/components/sections/*"],
      "@/molecules/*": ["./src/components/molecules/*"],
      "@/organisms/*": ["./src/components/organisms/*"]
    }
  }
}
